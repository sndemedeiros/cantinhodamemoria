{
    "builds": [
        {
            "src": "frontend/**/*.html",
            "use": "@vercel/static-build"
        },
        {
            "src": "frontend/**/*.js",
            "use": "@vercel/static-build"
        },
        {
            "src": "frontend/**/*.css",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}